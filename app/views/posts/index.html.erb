<div class="container">
  <div class="row">
  </div>
  <div class="row">
    <table class="table table-hover">
      <thead class="thead-dark">
        <tr>
          <th>Replied Count</th>
          <th>Title</th>
          <th>Last Replied</th>
          <th>Viewed Count</th>
        </tr>
      </thead>
      <tbody>
        <% @posts.each do |post| %>
          <tr>
            <td>Replied</td>
            <td><%= link_to post.title, post_path(post) %></td>
            <td>Last</td>
            <td>Viewed</td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>